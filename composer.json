{
    "name": "elisdn",
    "description": "",
    "type": "project",
    "require": {
        "php": ">=5.6.0",
        "symfony/dotenv": "^3.4",
        "yiisoft/yii": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "app": "php bin/yiic.php",
        "test": "phpunit --colors=always",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
